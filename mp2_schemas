    homepage smallint DEFAULT 1,
    homerecord integer,
    defaultrolecode character varying(10),
    defaultrole character varying(120),
    defaultrate double precision DEFAULT 0,
    homemode smallint,
    timesheet boolean DEFAULT true,
    holiday double precision DEFAULT 25,
    illness double precision DEFAULT 4,
    overhead double precision DEFAULT 10,
    other double precision DEFAULT 0,
    simpleproject boolean DEFAULT false,
    active boolean DEFAULT true NOT NULL
);


